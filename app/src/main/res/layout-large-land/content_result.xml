<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="io.github.leo3418.ctscan.ResultActivity"
    tools:showIn="@layout/activity_result" >

    <RelativeLayout
        android:layout_width="750dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center" >

        <TextView android:id="@+id/horizontal"
            android:text="@string/horizontal"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/topleft_bottomright"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/horizontal0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@id/horizontal"
            android:layout_alignStart="@id/horizontal"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/horizontal1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@id/horizontal0"
            android:layout_alignTop="@+id/horizontal0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/horizontal2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/horizontal1"
            android:layout_alignStart="@+id/horizontal0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/horizontal3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@+id/horizontal2"
            android:layout_below="@+id/horizontal1"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topleft_bottomright"
            android:text="@string/topleft_bottomright"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@id/horizontal2"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/topleft_bottomright0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/topleft_bottomright"
            android:layout_alignStart="@+id/topleft_bottomright"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topleft_bottomright1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@id/topleft_bottomright0"
            android:layout_alignTop="@+id/topleft_bottomright0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topleft_bottomright2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/topleft_bottomright1"
            android:layout_alignStart="@+id/topleft_bottomright0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topleft_bottomright3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@+id/topleft_bottomright2"
            android:layout_below="@+id/topleft_bottomright1"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/vertical"
            android:text="@string/vertical"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/topleft_bottomright"
            android:layout_below="@id/topleft_bottomright2"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/vertical0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@id/vertical"
            android:layout_alignStart="@id/vertical"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/vertical1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@id/vertical0"
            android:layout_alignTop="@+id/vertical0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/vertical2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/vertical1"
            android:layout_alignStart="@+id/vertical0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/vertical3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@+id/vertical2"
            android:layout_below="@+id/vertical1"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topright_bottomleft"
            android:text="@string/topright_bottomleft"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/horizontal"
            android:layout_below="@id/horizontal2"
            android:textAlignment="textEnd"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/topright_bottomleft0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@id/topright_bottomleft"
            android:layout_toStartOf="@id/topright_bottomleft1"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topright_bottomleft1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/topright_bottomleft0"
            android:layout_alignEnd="@id/topright_bottomleft"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topright_bottomleft2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/topright_bottomleft1"
            android:layout_alignStart="@+id/topright_bottomleft0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topright_bottomleft3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@+id/topright_bottomleft2"
            android:layout_below="@+id/topright_bottomleft1"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/overall"
            android:text="@string/overall"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:layout_toStartOf="@id/background"
            android:layout_alignBaseline="@id/topright_bottomleft"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/overall0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@id/overall"
            android:layout_alignStart="@id/overall"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/overall1"
            android:layout_height="50dp"
            android:layout_toEndOf="@id/overall0"
            android:layout_alignTop="@+id/overall0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/overall2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/overall1"
            android:layout_alignStart="@+id/overall0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/overall3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@+id/overall2"
            android:layout_below="@+id/overall1"
            android:layout_width="54dp"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/background"
            android:text="@string/background"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:layout_toStartOf="@id/result"
            android:layout_alignBaseline="@id/topright_bottomleft"
            android:layout_below="@id/vertical2"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/background0"
            android:layout_height="50dp"
            android:layout_below="@id/background"
            android:layout_alignStart="@id/background"
            android:layout_width="54dp"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/background1"
            android:layout_height="50dp"
            android:layout_toEndOf="@id/background0"
            android:layout_alignTop="@+id/background0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/background2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/background1"
            android:layout_alignStart="@+id/background0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/background3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toEndOf="@+id/background2"
            android:layout_below="@+id/background1"
            android:layout_width="54dp"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/result"
            android:text="@string/result"
            android:layout_width="108dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignBaseline="@id/topright_bottomleft"
            android:layout_below="@id/vertical2"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/result0"
            android:layout_height="50dp"
            android:layout_below="@id/result"
            android:layout_toStartOf="@id/result1"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/result1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignEnd="@id/result"
            android:layout_alignTop="@+id/result0"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/result2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/result1"
            android:layout_alignStart="@+id/result0"
            android:layout_width="54dp"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/result3"
            android:layout_height="50dp"
            android:layout_toEndOf="@+id/result2"
            android:layout_below="@+id/result1"
            android:layout_width="54dp"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

    </RelativeLayout>

</ScrollView>
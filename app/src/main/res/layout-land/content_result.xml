<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="io.github.leo3418.ctscan.ResultActivity"
    tools:showIn="@layout/activity_result">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView android:id="@+id/horizontal"
            android:text="@string/horizontal"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_toStartOf="@+id/vertical"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/horizontal02"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_below="@+id/horizontal"
            android:layout_alignStart="@+id/horizontal"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/horizontal13"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_toEndOf="@+id/horizontal02"
            android:layout_alignTop="@+id/horizontal02"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topleft_bottomright"
            android:text="@string/topleft_bottomright"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_below="@+id/horizontal02"
            android:layout_alignStart="@+id/horizontal"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/topleft_bottomright02"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_below="@+id/topleft_bottomright"
            android:layout_alignStart="@+id/topleft_bottomright"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topleft_bottomright13"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_toEndOf="@+id/topleft_bottomright02"
            android:layout_alignTop="@+id/topleft_bottomright02"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/vertical"
            android:text="@string/vertical"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_toStartOf="@+id/overall"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/vertical02"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_below="@+id/vertical"
            android:layout_alignStart="@+id/vertical"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/vertical13"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_toEndOf="@+id/vertical02"
            android:layout_alignTop="@+id/vertical02"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topright_bottomleft"
            android:text="@string/topright_bottomleft"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_below="@+id/vertical02"
            android:layout_toEndOf="@+id/topleft_bottomright"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/topright_bottomleft02"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_below="@+id/topright_bottomleft"
            android:layout_alignStart="@+id/topright_bottomleft"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/topright_bottomleft13"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_toEndOf="@+id/topright_bottomleft02"
            android:layout_alignTop="@+id/topright_bottomleft02"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/overall"
            android:text="@string/overall"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_alignBaseline="@+id/vertical02"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/overall02"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_below="@id/overall"
            android:layout_alignStart="@+id/overall"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/overall13"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_toEndOf="@+id/overall02"
            android:layout_alignTop="@+id/overall02"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/background"
            android:text="@string/background"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/overall"
            android:layout_alignBottom="@+id/overall"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/background02"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_below="@+id/background"
            android:layout_alignStart="@+id/background"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/background13"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_toEndOf="@+id/background02"
            android:layout_alignTop="@+id/background02"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/result"
            android:text="@string/result"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/background"
            android:layout_alignBottom="@+id/background"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView android:id="@+id/result02"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_below="@+id/result"
            android:layout_alignStart="@+id/result"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        <TextView android:id="@+id/result13"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_toEndOf="@+id/result02"
            android:layout_alignTop="@+id/result02"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

    </RelativeLayout>

</ScrollView>